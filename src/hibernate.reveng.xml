<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
<type-mapping>
	<sql-type jdbc-type="INTEGER" hibernate-type="int"
		not-null="true">
	</sql-type>
	<sql-type jdbc-type="VARCHAR" hibernate-type="string"
		not-null="true">
	</sql-type>
	<sql-type jdbc-type="VARCHAR" hibernate-type="string" length="12"
		not-null="true">
	</sql-type>
	<sql-type jdbc-type="VARCHAR" hibernate-type="string" length="10"
		not-null="true">
	</sql-type>
	<sql-type jdbc-type="VARCHAR" hibernate-type="string" length="10"
		not-null="false">
	</sql-type>
	<sql-type jdbc-type="CHAR" hibernate-type="java.lang.Character"
		length="1" not-null="false">
	</sql-type>
	<sql-type jdbc-type="VARCHAR" hibernate-type="string" length="8"
		not-null="false">
	</sql-type>
	<sql-type jdbc-type="VARCHAR" hibernate-type="string" length="45"
		not-null="false">
	</sql-type>
</type-mapping>









<table-filter match-catalog="VoceAluga" match-name="Carro" />
	<table-filter match-name=".*"></table-filter>
	<table-filter match-name="Pessoa" match-catalog="VoceAluga"></table-filter>
</hibernate-reverse-engineering>